{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "build": "tsc",
    "package": "ncc build --source-map --license LICENSE lib/main.js",
    "package-post": "ncc build lib/post.js -o dist/post",
    "dist": "npm run build && npm run package && npm run package-post",
    "format": "prettier --write src/*.ts src/**/*.ts src/**/**/*.ts",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bxb100/issueblog.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.6.0",
    "@actions/exec": "1.1.0",
    "@actions/github": "5.0.0",
    "handlebars": "4.7.7",
    "zod": "3.11.6"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/node": "16.11.11",
    "@typescript-eslint/parser": "5.5.0",
    "@vercel/ncc": "0.33.0",
    "eslint": "8.7.0",
    "eslint-plugin-github": "4.3.5",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.2"
  }
}
