{
  "name": "action-issue-blog",
  "version": "1.1.0",
  "private": true,
  "description": "Using GitHub Action and issue to create blog",
  "main": "lib/main.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "build": "tsc",
    "package": "ncc build --source-map --license LICENSE lib/main.js",
    "package-post": "ncc build lib/post.js -o dist/post",
    "dist": "npm run build && npm run package && npm run package-post",
    "format": "prettier --write src/*.ts src/**/*.ts src/**/**/*.ts",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bxb100/issueblog.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "handlebars": "4.7.7",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/parser": "^5.43.0",
    "@vercel/ncc": "^0.36.0",
    "eslint": "8.38.0",
    "eslint-plugin-github": "^4.4.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  }
}
